@model ReRegistering
<div class="modal fade" id="subscr_list_modal">
    <div class="modal-dialog">
        <div class="modal-content" style="width:880px;">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">გადაფორმება</h4>
            </div>
            <div class="modal-body">
                <table class="table table-striped table-bordered table-hover">
                    <thead>
                        <tr>

                            <th style="width:20%;">
                                თარიღი
                            </th>
                            <th style="width:20%;">
                                სახელი
                            </th>
                            <th style="width:20%;">
                                გვარი
                            </th>
                            <th style="width:20%;">
                                ტელეფონი
                            </th>
                            <th style="width:20%;">
                                პ.ნ
                            </th>
                        </tr>
                    </thead>
                    <tbody id="services_body">
                        <tr>
                            <td>@Model.tdate</td>
                            <td>@Model.name</td>
                            <td>@Model.lastname</td>
                            <td>@Model.phone</td>
                            <td>@Model.code</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default btn-xs" data-dismiss="modal">დახურვა</button>
            </div>
        </div>
    </div>
</div>



