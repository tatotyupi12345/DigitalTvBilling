@section head {
    <script type="text/javascript">
        var retunedCardChart=@Html.Raw(ViewBag.ReturnedCards);
        var returnedCard =@Html.Raw(ViewBag.CardsReturned);
        var promo_change=@Html.Raw(ViewBag.Promo_Cahnge);
        var promo_count=@Html.Raw(ViewBag.Promo);
        var cards_chart_data = @Html.Raw(ViewBag.CardsData);
        var returned_card=@Html.Raw(ViewBag.Cancled);
        var damage_card=@Html.Raw(ViewBag.Damage);
        var order_poll = @Html.Raw(ViewBag.Poll);
