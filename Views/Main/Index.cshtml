        var monthly_deals = @Html.Raw(ViewBag.MonthlyDeals);
        var charges_and_payments = @Html.Raw(ViewBag.ChargesAndPayments);
        var packets_by_cards = @Html.Raw(ViewBag.PacketsByCards);
        var canceled_cards_count = @Html.Raw(ViewBag.CanceledCardsCount);
        var packets_by_cities = @Html.Raw(ViewBag.PacketsByCities);
        var cities = @Html.Raw(ViewBag.Cities);
        $(function () {
            $(".datepickers").datePickers({ prefix: "picker" });
        });
    </script>
    <script type="text/javascript" src="~/Static/Scripts/highcharts.js"></script>
    <script type="text/javascript" src="~/Static/Scripts/highcharts-3d.js"></script>
    <script type="text/javascript" src="~/Static/Scripts/View/main.js"></script>
    <style>
        table {
            border-collapse: collapse;
        }

        table, th, td {
            border: 1px solid black;
        }
    </style>
}
